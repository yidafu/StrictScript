  "version": "0.2.0",
  "configurations": [{
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "test"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "runtimeArgs": [
        // "--loader",
        // "ts-node/esm",
        "-r",
        "ts-node/register"
      ],
      "env": { "TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json" },
      "args": [
        "${workspaceFolder}/examples/base.ts"
      ]
    }
  ]
}